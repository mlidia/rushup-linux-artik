/dts-v1/;
#include "exynos3250.dtsi"

/ {
	model = "Samsung ARTIK5 board based on EXYNOS3250";
	compatible = "samsung,artik5", "samsung,exynos3250";

	chosen {
		bootargs = "console=ttySAC2,115200 vmalloc=256M ess_setup=0x46000000";
	};

	memory {
		reg = <0x40000000 0x1F800000>;
	};

	aliases {
		i2c8 = &i2c_gpio0;
		spi2 = &spi_gpio0;
	};

	fixed-rate-clocks {
		#address-cells = <1>;
		#size-cells = <0>;

		xrtcxti: clock@0 {
			compatible = "fixed-clock";
			reg = <0>;
			clock-frequency = <32768>;
			#clock-cells = <0>;
			clock-output-names = "xrtcxti";
		};

		xusbxti: clock@1 {
			compatible = "fixed-clock";
			clock-frequency = <24000000>;
			reg = <1>;
			#clock-cells = <0>;
			clock-output-names = "xusbxti";
		};

		xxti: clock@2 {
			compatible = "fixed-clock";
			reg = <2>;
			clock-frequency = <0>;
			#clock-cells = <0>;
			clock-output-names = "xxti";
			};

		xtcxo: clock@3 {
			compatible = "fixed-clock";
			reg = <3>;
			clock-frequency = <26000000>;
			#clock-cells = <0>;
			clock-output-names = "xtcxo";
		};

		audiocdclk: clock@4 {
			compatible = "fixed-clock";
			reg = <4>;
			clock-frequency = <0>;
			#clock-cells = <0>;
			clock-output-names = "audiocdclk";
		};
	};

	i2c@13860000 {
		status = "okay";
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#size-cells = <0>;

		s2mps14_pmic@66 {
			compatible = "samsung,s2mps14-pmic";
			reg = <0x66>;
			interrupts = <5 0 0>;
			interrupt-parent = <&gpx3>;
			pinctrl-names = "default";
			pinctrl-0 = <&pmic_irq &pm_wrsti>;
			/* RTC: wtsr/smpl */
                        wtsr_en = <1>; /* enable */
                        smpl_en = <1>; /* enable */
                        wtsr_timer_val = <3>;   /* 1000ms */
                        smpl_timer_val = <4>;   /* 500ms */
                        check_jigon = <0>; /* do not check jigon */
                        /* RTC: If it's first boot, reset rtc to 1/1/2014 12:00:00(Wed) */
                        init_time,sec = <0>;
                        init_time,min = <0>;
                        init_time,hour = <12>;
                        init_time,mday = <1>;
                        init_time,mon = <0>;
                        init_time,year = <114>;
                        init_time,wday = <3>;

			regulators {
				buck1_reg: BUCK1 {
					regulator-name = "vdd_mif";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1600000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				buck2_reg: BUCK2 {
					regulator-name = "vdd_arm";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1600000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				buck3_reg: BUCK3 {
					regulator-name = "vdd_int";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1600000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo13_reg: LDO13 {
					regulator-name = "vdd_zb_2.8";
					regulator-min-microvolt = <2400000>;
					regulator-max-microvolt = <2400000>;
					regulator-initial-mode = <1>;
				};

				ldo16_reg: LDO16 {
					regulator-name = "vdd_lcd_3.3";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-initial-mode = <1>;
				};

				ldo18_reg: LDO18 {
					regulator-name = "vcc_peri_device_2.8";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-initial-mode = <1>;
				};

				ldo19_reg: LDO19 {
					regulator-name = "vcc_avdd_1.8";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-initial-mode = <1>;
				};

				ldo20_reg: LDO20 {
					regulator-name = "vcc_peri_device_1.8";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-initial-mode = <1>;
				};

				ldo21_reg: LDO21 {
					regulator-name = "zbcore_1.25";
					regulator-min-microvolt = <1250000>;
					regulator-max-microvolt = <1250000>;
					regulator-initial-mode = <1>;
				};

				ldo23_reg: LDO23 {
					regulator-name = "vcc_peri_1.8";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-always-on;
				};

				ldo6_reg: LDO6 {
                                        regulator-name = "VDD_USB_AP_1.0V";
                                        regulator-min-microvolt = <1000000>;
                                        regulator-max-microvolt = <1000000>;
                                        regulator-always-on;
					regulator-initial-mode = <1>;
                                };

				ldo8_reg: LDO8 {
                                        regulator-name = "VDD_UOTG_AP_3.3V";
                                        regulator-min-microvolt = <3000000>;
                                        regulator-max-microvolt = <3000000>;
                                };

				ldo2_reg: LDO2 {
					regulator-always-on;
					regulator-initial-mode = <1>;
                                };

				ldo4_reg: LDO4 {
					regulator-always-on;
                                        regulator-initial-mode = <1>;
                                };

				ldo5_reg: LDO5 {
					regulator-always-on;
                                        regulator-initial-mode = <1>;
                                };

				ldo7_reg: LDO7 {
					regulator-always-on;
                                        regulator-initial-mode = <1>;
                                };
			};
		};
	};

	dwmmc0@12510000 {
		status = "okay";
		num-slots = <1>;
		broken-cd;
		bypass-smu;
		fixed_volt;
		only_once_tune;
		extra_tuning;
		supports-highspeed;
		enable-cache-control;
		sw_data_timeout;
		fifo-depth = <0x80>;
		card-detect-delay = <200>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 4 2>;
		samsung,dw-mshc-hs200-timing = <3 0 2 0>;
		samsung,dw-mshc-ddr200-timing = <1 1 2 0>;
		num-ref-clks = <8>;
		gpios = <&gpk0 2 0x1>;
		ciu_clkin = <25 50 50 100 200 100 200 200>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_bus1 &sd0_bus4 &sd0_bus8>;

		slot@0 {
			reg = <0>;
			bus-width = <8>;
		};
	};

	dwmmc1@12520000 {
		status = "okay";
		num-slots = <1>;
		channel = <1>;
		caps-control;
		bypass-smu;
		clock-gate;
		enable-cclk-on-suspend;
		fixed_volt;
		use-fine-tuning;
		supports-highspeed;
		sw_data_timeout;
		supports-sdr104-mode;
		keep-power-in-suspend;
		pm-ignore-notify;
		cd-type-external;
		cd-type = <2>;
		use-broken-voltage;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <222000>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 3 0>;
		samsung,dw-mshc-ddr-timing = <3 0 2 0>;
		clk_pin = "gpk1-0";
		clk_addr = "11000000.pinctrl";
		clk_val = <0x3>;
		clk_str_num = <4>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus1 &sd1_bus4>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};
	};

	dwmmc2@12530000 {
		status = "okay";
		num-slots = <1>;
		bypass-smu;
		supports-highspeed;
		fixed_volt;
		cd-type = <1>;
		fifo-depth = <0x80>;
		card-detect-delay = <200>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 4 2>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 200>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_cd &sd2_bus1 &sd2_bus4>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};
	};

        ion {
                compatible = "samsung,exynos5430-ion";

		contig-names = "mfc_sh",
				"video",
				"mfc_fw",
				"mfc_nfw";

		contig = <2 0x100000 0x0>,
			<11 0xA00000 0x0>,
			<8 0x100000 0x0>,
			<12 0x100000 0x0>;

                ion_noncontig_heap {
                        id-type = <0 0>;
                };

		exynos_contig_heap {
			id-type = <4 6>;
		};

        };

	i2c@13870000 {
	        samsung,i2c-sda-delay = <25>;
	        samsung,i2c-max-bus-freq = <400000>;
	        #address-cells = <1>;
	        #size-cells = <0>;
	        status = "okay";

	        CW2015@62 {
	                compatible = "cw201x";
	                reg = <0x62>;
	                /*      pinctrl-0 = <&cw_irq>; */
					bat_config_info = <0x15 0x4C 0x78 0x48 0x25 0x16 0x45 0x60 0x67 0x5D
                                       0x56 0x4C 0x58 0x58 0x53 0x57 0x59 0x5C 0x5E 0x65
                                       0x57 0x6C 0x78 0x75 0x85 0x91 0x07 0xAE 0x74 0xA4
                                       0x96 0xA3 0xC1 0xC2 0xD2 0xBF 0x40 0x1F 0x98 0x19
                                       0x05 0x38 0x48 0x79 0x8D 0x92 0x92 0x23 0x73 0x80
                                       0x94 0xB0 0x37 0xC1 0x91 0xCB 0x2F 0x7D 0x72 0xA5
                                       0xB5 0xC1 0x15 0x09>;
	                /*
	                 is_dc_charge = <1>;
	                 dc_det_gpio = <1>;
	                 is_usb_charge = <0>;
	                */
	        };
	};

	sysreg_localout: sysreg_localout@10010000 {
		compatible = "samsung,exynos5-sysreg_localout";
		reg = <0x10010000 0x2000>;
	};

	mdev_0: mdev_output {
		compatible = "samsung,exynos5-mdev";
	};

	usb@12480000 {
		vusb_d-supply = <&ldo6_reg>;
		vusb_a-supply = <&ldo8_reg>;
		status = "okay";
	};

	slp_multi: slp_multi {
		compatible = "slp,slp_multi";
		slp,slp_mult-nluns = <2>;
		slp,slp_mult-swfi-latency = <1>;
		slp,slp_mult-nfuncs = <4>;
		slp,slp_mult-enable-funcs = "rndis", "sdb", "mtp", "acm";
	};

	spi@13920000 {
		status = "okay";
			#address-cells = <1>;
			#size-cells = <0>;
	};

	serial@13800000 {
		force-auto-flow-control;
		status = "okay";
	};

	pinctrl@11000000 {
		cfg_wlanen: cfg-wlanen {
			samsung,pins = "gpx2-3";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <3>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <0>;
		};

		cfg_wlanhostwake: cfg-wlanhostwake {
			samsung,pins = "gpx3-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <0>;
		};

		cfg_en: cfg-en {
			samsung,pins ="gpx1-7";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <1>;
		};

		cfg_btwake: cfg-btwake {
			samsung,pins = "gpx3-7";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-con-pdn = <0>;
			samsung,pin-pud-pdn = <1>;
		};

		cfg_hostwake: cfg-hostwake {
			samsung,pins = "gpx3-6";
			samsung,pin-function = <0>;
			samsung,pin-pud = <3>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <0>;
		};

		zb_rstn: zb-rstn {
			samsung,pins = "gpm0-1";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-val = <1>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <1>;
		};

		zb_bootloader: zb-bootloader {
			samsung,pins = "gpe0-0";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-val = <1>;
			samsung,pin-drv = <3>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <1>;
		};

		xr20m1172_int: xm20m1172-int {
			samsung,pins = "gpx2-4";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};

		xr20m1172_reset: xm20m1172-int {
			samsung.pins = "gpx2-2";
			samsung,pin-function = <0x1>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
	};

	wlan {
		compatible = "samsung,brcm-wlan";
		gpios = <&gpx2 3 0x1	/* WLAN_EN */
			 &gpx3 2 0xF	/* WLAN_HOST_WAKE */
			>;
		pinctrl-names = "default";
		pinctrl-0=<&cfg_wlanen &cfg_wlanhostwake>;
		status = "okay";
	};

	bluetooth {
		compatible = "broadcom,bcm4354bt";

		gpios = <&gpx1 7 0	/* BT_EN */
			 &gpx3 7 0	/* BT_WAKE */
			 &gpx3 6 0xF	/* BT_HOST_WAKE */
			 >;

		pinctrl-names = "default";
		pinctrl-0=<&cfg_hostwake &cfg_btwake &cfg_en>;
		status = "okay";
	};

	gpio_keys {
		compatible = "gpio-keys";

		power_key {
			label = "power key";
			linux,code = <116>;
			gpios = <&gpx2 7 0x1>;
			gpio-key,wakeup = <1>;
			debounce-interval = <0>;
		};
	};

	i2c@13870000 {
		status = "okay";
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#size-cells = <0>;


		gaudio_codec: sgtl5000@0a {
			compatible = "fsl,sgtl5000";
			reg = <0x0a>;
			VDDA-supply = <&vddext_18>;
			VDDIO-supply = <&vddext_18>;
			VDDD-supply = <&vddext_18>;
		};

		bq24296_charger@6b {
			compatible = "ti,bq24296";
			reg = <0x6b>;
			ti,chg_current = <1000 500 2000>;
		};
	};

	i2s: i2s@13970000 {
		status = "okay";
	};

	sound {
		compatible = "samsung,artik-ak4953";
		samsung,audio-cpu = <&i2s>;
		samsung,audio-codec = <&gaudio_codec>;
		clocks = <&clock CLK_MOUT_EPLL>,
			 <&clock CLK_FOUT_EPLL>,
			 <&clock CLK_MOUT_AUDIO>,
			 <&clock CLK_DIV_AUDIO>,
			 <&clock CLK_DIV_I2S>,
			 <&clock CLK_SCLK_I2S>;
		clock-names = "mout_epll", "fout_epll",
			      "mout_audio", "dout_audio",
			      "dout_i2s", "sclk_i2s";
		VDDA-supply = <&vddext_18>;
		VDDIO-supply = <&vddext_18>;
		VDDD-supply = <&vddext_18>;
		status = "okay";
	};

	artik_zb_power {
		compatible = "samsung,artik_zb_power_v31";
		pinctrl-names = "default";
		pinctrl-0 = <&zb_rstn &zb_bootloader>;
		vdd_zb-supply = <&ldo13_reg>;
		vdd-supply = <&ldo18_reg>;
		vdd_zbcore-supply = <&ldo21_reg>;
		reset-gpio = <&gpm0 1 0x1>;
		bootloader-gpio = <&gpe0 0 0x1>;
	};

	i2c@13890000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#size-cells = <0>;
		status = "okay";

		touchscreen@38 {
			compatible = "focaltech,5x06";
			reg = <0x38>;
			interrupt-parent = <&gpx0>;
			interrupts = <6 0 0>;
			pinctrl-names = "default";
			pinctrl-0 = <&touch_irq>;
			focaltech,name = "ft5336";
			focaltech,family-id = <0x14>;
			focaltech,reset-gpio = <&gpx0 4 0x1>;
			focaltech,irq-gpio = <&gpx0 6 0x1>;
			focaltech,display-coords = <0 0 320 320>;
			focaltech,panel-coords = <0 0 320 320>;
			focaltech,button-map= <139 102 158>;
			focaltech,no-force-update;
			focaltech,i2c-pull-up;
			focaltech,group-id = <1>;
			focaltech,hard-reset-delay-ms = <20>;
			focaltech,soft-reset-delay-ms = <200>;
			focaltech,num-max-touches = <5>;
			#focaltech,fw-name = "ft_8926_qrd_fw.bin";
			focaltech,fw-delay-aa-ms = <30>;
			focaltech,fw-delay-55-ms = <30>;
			#focaltech,fw-upgrade-id1 = <0x79>;
			#focaltech,fw-upgrade-id2 = <0x08>;
			focaltech,fw-delay-readid-ms = <10>;
			focaltech,fw-delay-era-flsh-ms = <2000>;
			focaltech,fw-auto-cal;
			focaltech,ignore-id-check;
		};

	};

	regulators {
                compatible = "simple-bus";
                #address-cells = <1>;
                #size-cells = <0>;

		vddext_18: voltage-regulator-1 {
                        compatible = "regulator-fixed";
			regulator-name = "VDDEXT_18";
                        regulator-min-microvolt = <1800000>;
                        regulator-max-microvolt = <1800000>;
		};

		vddext_28: voltage-regulator-2 {
                        compatible = "regulator-fixed";
			regulator-name = "VDDEXT_28";
                        regulator-min-microvolt = <2800000>;
                        regulator-max-microvolt = <2800000>;
		};
	};

	i2c_gpio0: i2c@3c {
		compatible = "i2c-gpio";
		gpios = <&gpm4 1 0 &gpm4 0 0>;
		i2c-gpio,delay-us = <2>;    /* ~100kHz */
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		s5k4ec@56 {
			compatible = "samsung,exynos5-fimc-is-sensor-4ec";
			reg = <0x56>;
		};

		ov5642@2d {
			compatible = "samsung,exynos5-fimc-is-sensor-ov5640";
			reg = <0x3c>;
		};
	};

	fimc_is@14000000 {
		fixed_sensor_id = <202>;
		fimc_is_dvfs {
			default_int = <134000>; /* L1 */
			default_cam = <320000>; /* L0 */
			default_mif = <400000>; /* L0 */
			default_i2c = <0>;

			max_int = <134000>; /* L1 */
			max_cam = <320000>; /* L0 */
			max_mif = <400000>; /* L0 */
			max_i2c = <0>;
		};
	};

	fimc_is_sensor0: fimc_is_sensor@120A0000 {
		scenario = <2>;
		gpio_reset = <&gpm3 2 0x1>;
		gpios_cam_en = <&gpm0 2 0x1>;
		gpios_comp_en = <&gpx2 6 0x1>;
		id = <0>;
		mclk_ch = <0>;
		csi_ch = <0>;
		flite_ch = <0>;
		i2c_ch = <0>;
		i2c_addr = <0x3c>;
		is_bns = <0>;
		status = "okay";
	};

	spi_gpio0: spi-gpio0 {
		compatible = "spi-gpio";
		status = "okay";
		#address-cells = <1>;
		#size-cells = <0>;
		spi-max-frequency = <4000000>;
		gpio-sck = <&gpm3 4 0>;
		gpio-mosi = <&gpm3 5 0>;
		gpio-miso = <&gpm3 6 0>;
		cs-gpios = <&gpm3 7 0>;
		num-chipselects = <1>;

		xr20m1172 {
			compatible = "exar,xrm117x";
			interrupt-parent = <&gpx2>;
			interrupts = <4 0 0>;
			pinctrl-names = "default";
			pinctrl-0 = <&xr20m1172_int &xr20m1172_reset>;
			spi-max-frequency = <4000000>;
			reg = <0>;
		};
	};
};

